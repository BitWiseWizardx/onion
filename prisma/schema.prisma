generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model incomes {
  id          Int      @id @default(autoincrement())
  description String   @db.VarChar(255)
  amount      String   @db.VarChar(255)
  created_at  DateTime @default(now()) @db.DateTime(0)
}

model expenses {
  id          Int      @id @default(autoincrement())
  description String   @db.VarChar(255)
  quantity Int
  amount      String   @db.VarChar(255)
  created_at  DateTime @default(now()) @db.DateTime(0)
}